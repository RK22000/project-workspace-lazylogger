<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.control.LoggerReportController2">
   <children>
      <Button mnemonicParsing="false" onAction="#openLoggerWidget" text="Logger Widget" />
      <HBox alignment="CENTER" fillHeight="false" maxWidth="1.7976931348623157E308" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#focusToPrevWeek" text="&lt;= Pervious week" />
            <Label text="Week of " />
            <DatePicker fx:id="focusPicker" onAction="#focusOnWeek" />
            <Button mnemonicParsing="false" onAction="#focusToNextWeek" text="Next Week =&gt;" />
         </children>
      </HBox>
      <TableView fx:id="LogTable" maxWidth="1.7976931348623157E308">
        <columns>
          <TableColumn fx:id="TagColumn" editable="false" prefWidth="300.0" sortable="false" text="Job Activities" />
        </columns>
         <cursor>
            <Cursor fx:constant="CROSSHAIR" />
         </cursor>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TableView>
      <Label fx:id="LogDisplay" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Label" />
   </children>
</VBox>
