<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.control.LoggerReportController">
   <children>
      <Button mnemonicParsing="false" onAction="#openLoggerWidget" text="Return" />
      <HBox>
         <children>
            <VBox fx:id="ActivityBox" prefWidth="200.0" />
            <ScrollPane fx:id="HScrollBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <content>
                  <HBox fx:id="DayLogBox" prefHeight="100.0" prefWidth="200.0" />
               </content>
            </ScrollPane>
         </children>
      </HBox>
      <Label fx:id="displayBox" text="Label" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
